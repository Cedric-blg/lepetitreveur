<form method="post" action="{{ path('app_presence_delete', {'id': presence.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette présence?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ presence.id) }}">
    <button class="btn">Supprimer</button>
</form>
